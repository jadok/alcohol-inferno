{
  "presets": [
    "es2015-rollup"
  ],
  "plugins": [
    "external-helpers",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-object-rest-spread",
    "babel-plugin-syntax-jsx",
    ["babel-plugin-inferno", {"imports": true}],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ]
  ]
}
