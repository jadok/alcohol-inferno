{
  "name": "inferno-typescript-example",
  "version": "1.0.0",
  "description": "A basic example of Inferno with TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/sitespeed.io/bin/sitespeed.js http://localhost:5000",
    "build": "webpack -p",
    "lint": "tslint src/*.ts{,x} src/**/*.ts{,x}",
    "start": "webpack-dev-server --env.dev --watch --config ./config/webpack.dev.js",
    "serve": "NODE_ENV=production webpack -p --config ./config/webpack.prod.js && grunt sass",
    "deploy": "npm run serve && serve ./dist/"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "keywords": [
    "inferno",
    "typescript"
  ],
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "Quentin Jadeau",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.1.0",
    "eslint-plugin-es6": "^1.0.0",
    "grunt-contrib-htmlmin": "^2.3.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^2.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "inferno": "^3.6.0",
    "inferno-component": "^3.6.0",
    "inferno-devtools": "^3.9.0",
    "inferno-redux": "^3.9.0",
    "inferno-router": "^3.6.0",
    "inferno-shared": "^3.6.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-inferno-app": "^5.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cssstats": "^3.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "grunt": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "inferno-test-utils": "^3.9.0",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "sitespeed.io": "^5.5.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.2.0",
    "tslint": "^5.1.0",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.4.0",
    "typescript-eslint-parser": "^7.0.0",
    "webpack": "^2.3.3",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "^4.1.0"
  }
}
